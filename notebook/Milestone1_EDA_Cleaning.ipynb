        "# -------------------- CLEAN + MERGE PERSON DATA --------------------\n",
        "\n",
        "# Ensure COLLISION_ID is string for merging\n",
        "df_crashes_cleaned['COLLISION_ID'] = df_crashes_cleaned['COLLISION_ID'].astype(str)\n",
        "\n",
        "persons_file = \"Motor_Vehicle_Collisions_-_Person_20251112.csv\"\n",
        "print(f\"Loading Person Data from: {persons_file}...\")\n",
        "\n",
        "# -------------------- 1. Load & Clean PERSON Dataset --------------------\n",
        "try:\n",
        "    df_person = pd.read_csv(persons_file, low_memory=False)\n",
        "\n",
        "    # Remove rows with no collision ID\n",
        "    df_person = df_person.dropna(subset=[\"COLLISION_ID\"])\n",
        "    df_person[\"COLLISION_ID\"] = df_person[\"COLLISION_ID\"].astype(str)\n",
        "\n",
        "    # Normalize PERSON_SEX (fixes your UNKNOWN problem!)\n",
        "    df_person[\"PERSON_SEX_L\"] = (\n",
        "        df_person[\"PERSON_SEX\"]\n",
        "        .astype(str)\n",
        "        .str.upper()\n",
        "        .str.strip()\n",
        "        .replace({\n",
        "            \"MALE\": \"M\",\n",
        "            \"M\": \"M\",\n",
        "            \"FEMALE\": \"F\",\n",
        "            \"F\": \"F\",\n",
        "            \"FE\": \"F\",\n",
        "            \"MA\": \"M\",\n",
        "            \"U\": np.nan,\n",
        "            \"UNK\": np.nan,\n",
        "            \"UNKNOWN\": np.nan,\n",
        "            \"NAN\": np.nan,\n",
        "            \"\": np.nan,\n",
        "            \" \": np.nan\n",
        "        })\n",
        "    )\n",
        "\n",
        "    # Normalize PERSON_INJURY\n",
        "    df_person[\"PERSON_INJURY_L\"] = (\n",
        "        df_person[\"PERSON_INJURY\"]\n",
        "        .astype(str)\n",
        "        .str.lower()\n",
        "        .str.strip()\n",
        "    )\n",
        "\n",
        "except FileNotFoundError:\n",
        "    print(f\" ERROR  File not found: {persons_file}\")\n",
        "    df_person = pd.DataFrame(columns=[\"COLLISION_ID\"])\n",
        "\n",
        "print(\"Aggregating Person Data...\")\n",
        "\n",
        "# -------------------- 2. Correct Injury / Killed Flags --------------------\n",
        "df_person[\"IS_KILLED\"] = (df_person[\"PERSON_INJURY_L\"] == \"killed\").astype(int)\n",
        "df_person[\"IS_INJURED\"] = (df_person[\"PERSON_INJURY_L\"] == \"injured\").astype(int)\n",
        "\n",
        "# -------------------- 3. Group the Person Table by Collision --------------------\n",
        "person_summary = df_person.groupby(\"COLLISION_ID\").agg(\n",
        "    PERSONS_INVOLVED_COUNT=(\"PERSON_ID\", \"count\"),\n",
        "    KILLED_COUNT_PERSON=(\"IS_KILLED\", \"sum\"),\n",
        "    INJURED_COUNT_PERSON=(\"IS_INJURED\", \"sum\"),\n",
        "    \n",
        "    # FIX: Get the most common REAL value (\"M\" or \"F\")\n",
        "    MOST_COMMON_SEX=(\n",
        "        \"PERSON_SEX_L\", \n",
        "        lambda x: x.dropna().mode()[0] if len(x.dropna()) else np.nan\n",
        "    )\n",
        ").reset_index()\n",
        "\n",
        "print(\"Merging crash and person datasets...\")\n",
        "\n",
        "# -------------------- 4. Merge datasets --------------------\n",
        "merged = pd.merge(\n",
        "    df_crashes_cleaned,\n",
        "    person_summary,\n",
        "    on=\"COLLISION_ID\",\n",
        "    how=\"left\"\n",
        ")\n",
        "\n",
        "print(f\"Integrated dataset shape: {merged.shape}\")\n",
        "\n",
        "# -------------------- 5. Final Cleanup --------------------\n",
        "\n",
        "# Sex cleanup  now UNKNOWN means truly missing\n",
        "merged[\"MOST_COMMON_SEX\"] = merged[\"MOST_COMMON_SEX\"].fillna(\"UNKNOWN\")\n",
        "\n",
        "# Fill numerical counts\n",
        "for col in [\"PERSONS_INVOLVED_COUNT\", \"KILLED_COUNT_PERSON\", \"INJURED_COUNT_PERSON\"]:\n",
        "    merged[col] = merged[col].fillna(0).astype(int)\n",
        "\n",
        "# Combine CRASH table + PERSON table injury/killed counts\n",
        "merged[\"NUMBER OF PERSONS INJURED\"] = np.maximum(\n",
        "    merged[\"NUMBER OF PERSONS INJURED\"].fillna(0),\n",
        "    merged[\"INJURED_COUNT_PERSON\"]\n",
        ")\n",
        "\n",
        "merged[\"NUMBER OF PERSONS KILLED\"] = np.maximum(\n",
        "    merged[\"NUMBER OF PERSONS KILLED\"].fillna(0),\n",
        "    merged[\"KILLED_COUNT_PERSON\"]\n",
        ")\n",
        "\n",
        "# Remove temporary columns\n",
        "merged = merged.drop(columns=[\"INJURED_COUNT_PERSON\", \"KILLED_COUNT_PERSON\"])\n",
        "\n",
        "# Clean Borough Column\n",
        "if \"BOROUGH\" in merged.columns:\n",
        "    merged[\"BOROUGH\"] = merged[\"BOROUGH\"].astype(str).str.upper().replace(\"UNKNOWN\", np.nan)\n",
        "\n",
        "print(\"\\nPost-Integration Cleaning Complete.\")\n",
        "\n",
        "# -------------------- Save Output --------------------\n",
        "output_file_name = \"merged_crashes_person.csv\"\n",
        "merged.to_csv(output_file_name, index=False)\n",
        "print(f\" Final cleaned dataset saved to: {output_file_name}\")\n"
      ]
